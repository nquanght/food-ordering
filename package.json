{
  "name": "food-ordering",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js",
    "migrate:make": "knex migrate:make $npm_config_migration_name --migrations-directory=./src/migrations",
    "migrate:rollback": "knex migrate:rollback --knexfile ./src/config/database.js --env development",
    "migrate": "knex migrate:latest --knexfile ./src/config/database.js --env development",
    "seed:make": "knex seed:make $npm_config_seeder_name --knexfile ./src/config/database.js",
    "seed:run": "knex seed:run --knexfile ./src/config/database.js"
  },
  "author": "wank-it",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.7.7",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "knex": "^3.1.0",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  }
}
